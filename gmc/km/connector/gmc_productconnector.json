      }
    },
    {
      "findAndReplace": {
        "inputHeaders": {
          "headers": [
            "Energy Efficiency Class",
            "Minimum Energy Efficiency Class",
            "Maximum Energy Efficiency Class"
          ]
        },
        "valueToFind": {
          "plainText": {
            "value": "+",
            "ignoreCase": false
          }
        },
        "replacement": "_PLUS"
      }
    },
    {
      "extractText": {
        "inputHeader": "Sale Price Date (raw)",
        "newColumnHeader": "Sale Price Start Date (raw)",
        "extractOperation": "ALL_TEXT_BEFORE",
        "extractFrom": "FIRST_INSTANCE_MATCHING_TEXT",
        "valueToFind": {
          "plainText": {
            "value": "T",
            "ignoreCase": false
          }
        }
      }
    },
    {
      "extractText": {
        "inputHeader": "Sale Price Date (raw)",
        "newColumnHeader": "Sale Price End Date (raw)",
        "extractOperation": "SOME_TEXT_AFTER",
        "extractFrom": "FIRST_INSTANCE_MATCHING_TEXT",
        "valueToFind": {
          "plainText": {
            "value": "/",
            "ignoreCase": false
          }
        },
        "maxLengthTextToKeep": 10
      }
    },
    {
      "fixCapitalization": {
        "inputHeaders": {
          "headers": [
            "Pickup Method"
          ]
        },
        "cleanOption": "ALL_CAPS"
      }
    },
    {
      "findAndReplace": {
        "inputHeaders": {
          "headers": [
            "Pickup Method"
          ]
        },
        "valueToFind": {
          "plainText": {
            "value": " ",
            "ignoreCase": false
          }
        },
        "replacement": "_"
      }
    }
  ],
